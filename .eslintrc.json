{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb",
		"plugin:react/recommended"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"eol-last": 0,
		"linebreak-style": 0,
		"object-curly-newline": 0,
		"react/sort-comp": 0,
		"react/forbid-prop-types": 0,
		"react/require-default-props": 0,
		"react/jsx-indent-props": 0,
		"react/static-property-placement": 0,
		"react/state-in-constructor": 0,
		"react/jsx-props-no-spreading": 0,
		"jsx-a11y/anchor-is-valid": 0,
		"react/jsx-indent": [
			"error",
			4
		],
		"indent": [
			"error",
			4,
			{
				"SwitchCase": 1
			}
		],
		"no-console": 0,
		"brace-style": [
			"error",
			"stroustrup"
		]
	}
}